+  load_time_data.Set("finalParagraph", "");
+  load_time_data.Set("optInLink", "optInLink");
+
+  load_time_data.Set("enhancedProtectionMessage", "");
+
+  load_time_data.Set("hide_primary_button", false);
+  load_time_data.Set("primaryButtonText",
