[
    Exposed=(Window,Worker),
    RuntimeEnabled=RTCRtpTransport
] interface RtcReceivedPacket {
  readonly attribute unsigned long payloadByteLength;
  [RaisesException] void copyPayloadTo(AllowSharedBufferSource destination);
  readonly attribute DOMHighResTimeStamp receiveTime;
};
