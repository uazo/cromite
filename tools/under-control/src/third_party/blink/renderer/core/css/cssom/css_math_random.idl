// Represents CSS random() function.
[
  Exposed=(Window,LayoutWorklet,PaintWorklet),
  RuntimeEnabled=CSSRandomFunctionTypedOM
] interface CSSMathRandom : CSSMathValue {
  [RaisesException] constructor(double baseValue, CSSNumberish min, CSSNumberish max, optional CSSNumberish? step = null);
  readonly attribute double baseValue;
  readonly attribute CSSNumericValue min;
  readonly attribute CSSNumericValue max;
  readonly attribute CSSNumericValue? step;
};
