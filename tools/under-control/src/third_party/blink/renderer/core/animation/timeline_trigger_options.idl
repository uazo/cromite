// https://drafts.csswg.org/web-animations-2/#the-animationtriggeroptions-dictionary

dictionary TimelineTriggerOptions {
  AnimationTimeline? timeline;
  (DOMString or TimelineRangeOffset or undefined) activationRangeStart;
  (DOMString or TimelineRangeOffset or undefined) activationRangeEnd;
  (DOMString or TimelineRangeOffset or undefined) activeRangeStart;
  (DOMString or TimelineRangeOffset or undefined) activeRangeEnd;
};
