// https://html.spec.whatwg.org/C/#submitevent

[
  Exposed=Window
] interface SubmitEvent : Event {
  constructor(DOMString type, optional SubmitEventInit eventInitDict = {});
  readonly attribute HTMLElement? submitter;
  [RuntimeEnabled=WebMCP] readonly attribute boolean agentInvoked;
  [CallWith=ScriptState, RaisesException, RuntimeEnabled=WebMCP] void respondWith(Promise<any> agentResponse);
};
